.batar-childPage
  position fixed
  z-index 100
  left 0
  top 0
  width 100% 
  height 100%
  background #fff
.batar-changePage-enter-active,.batar-changePage-leave-active
  transition all 0.2s cubic-bezier(0, 0, .58, 1)
.batar-changePage-enter,.batar-changePage-leave-to
  -webkit-transform translate(100%, 0)

@font-face {
  font-family: 'iconfont';  /* project id 474509 */
  src: url('//at.alicdn.com/t/font_474509_1czql3w99f1rlik9.eot');
  src: url('//at.alicdn.com/t/font_474509_1czql3w99f1rlik9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_474509_1czql3w99f1rlik9.woff') format('woff'),
  url('//at.alicdn.com/t/font_474509_1czql3w99f1rlik9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_474509_1czql3w99f1rlik9.svg#iconfont') format('svg');
}
.iconfont
  font-family "iconfont" !important
  font-size 0.14rem
  font-style normal 
  -webkit-font-smoothing antialiased
  -webkit-text-stroke-width .002rem 
  -moz-osx-font-smoothing grayscale


